{
  "name": "mcp-client",
  "version": "0.1.0",
  "description": "Command-line MCP client for the Local DeepReasoning server",
  "main": "build/index.js",
  "type": "commonjs",
  "exports": {
    ".": "./build/index.js",
    "./lib": {
      "types": "./build/lib.d.ts",
      "require": "./build/lib.js",
      "default": "./build/lib.js"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node build/index.js",
    "lint": "tsc --noEmit",
    "link": "npm run build && npm link"
  },
  "bin": {
    "mcp-client": "build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
